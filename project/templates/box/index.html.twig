{% extends 'base.html.twig' %}

{% block title %}Box{% endblock %}

{% block body %}

<table class="table table-sm">
    <thead>
        <th>Objet</th>
        <th>Categorie</th>
        <th>Carton</th>
    </thead>

    <tbody>
    {% for objet in objets %}
        <tr>
            <td>{{ objet.nom }}</td>
            <td>{{ objet.categorie.nom }}</td>
            <td>{{ objet.carton.numero }}</td>
        </tr>
    {% endfor %}
    </tbody>

</table>

{% endblock %}